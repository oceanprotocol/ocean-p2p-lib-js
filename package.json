    "@libp2p/ping": "^1.1.1",
    "@libp2p/pubsub": "^9.0.22",
    "@libp2p/pubsub-peer-discovery": "^10.0.2",
    "@libp2p/tcp": "^9.1.1",
    "@libp2p/upnp-nat": "^1.2.1",
    "@libp2p/websockets": "^8.1.1",
