    "eslint": "^8.50.0",
    "eslint-config-oceanprotocol": "^2.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "lodash.clonedeep": "^4.5.0"
